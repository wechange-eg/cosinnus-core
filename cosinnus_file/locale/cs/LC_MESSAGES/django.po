msgstr "Složka \"%(title)s\" byla úspěšně vytvořena."

msgid "File does not exist or you are not allowed to delete it."
msgstr "Soubor neexistuje nebo jej nesmíte smazat."

#, python-format
msgid "A problem occurred during export. The following file is missing: %(filename)s."
msgid_plural "A problem occurred during export. The following files are missing: %(filename)s."
msgstr[0] "Během exportu došlo k problému. Chybí následující soubor: %(filename)s."
msgstr[1] "Během exportu došlo k problému. Chybí následující soubory: %(filename)s."
msgstr[2] "Během exportu došlo k problému. Chybí následující soubory: %(filename)s."
msgstr[3] "Během exportu došlo k problému. Chybí následující soubory: %(filename)s."

msgid "Invalid request."
msgstr "Neplatný požadavek."

#, python-format
msgid "File \"%(title)s\" was updated successfully."
msgstr "Soubor \"%(title)s\" byl úspěšně aktualizován."

#, python-format
msgid "File \"%(title)s\" could not be updated."
msgstr "Soubor \"%(title)s\" nelze aktualizovat."

msgid "Downloading failed, the target object was not a folder!"
msgstr "Stahování se nezdařilo, cílovým objektem nebyla složka!"

msgid "Canceled the download because there seem to be no files in this folder."
msgstr "Stahování bylo zrušeno, protože se zdá, že v této složce nejsou žádné soubory."

msgid "Internal Error: Group not Found"
msgstr "Interní chyba: Skupina nenalezena"

msgid "Permission for upload denied"
msgstr "Povolení pro nahrání odmítnuto"

msgid "The uploaded file was invalid."
msgstr "Nahraný soubor byl neplatný."

#, python-format
msgid "%(count)d File was added successfully."
msgid_plural "%(count)d Files were added successfully."
msgstr[0] "%(count)d soubor byl úspěšně přidán."
msgstr[1] "%(count)d soubory byly úspěšně přidány."
msgstr[2] "%(count)d soubory byly úspěšně přidány."
msgstr[3] "%(count)d souborů byly úspěšně přidáno."

msgid "The file you uploaded was too large or the file type was not permitted to be uploaded!"
msgstr "Soubor, který jste nahrál, byl příliš velký nebo pro tento typ souboru nebylo povoleno nahrání!"
