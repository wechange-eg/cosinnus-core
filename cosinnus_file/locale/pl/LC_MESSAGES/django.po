msgstr "Folder \"%(title)s\" został pomyślnie utworzony."

msgid "File does not exist or you are not allowed to delete it."
msgstr "Plik nie istnieje lub nie masz uprawnień, aby go usunąć."

#, python-format
msgid "A problem occurred during export. The following file is missing: %(filename)s."
msgid_plural "A problem occurred during export. The following files are missing: %(filename)s."
msgstr[0] "Podczas eksportu wystąpił problem. Brakuje następującego pliku: %(filename)s."
msgstr[1] "Podczas eksportu wystąpił problem. Brakuje następujących plików: %(filename)s."
msgstr[2] "Podczas eksportu wystąpił problem. Brakuje następujących plików: %(filename)s."

msgid "Invalid request."
msgstr "Nieprawidłowe żądanie."

#, python-format
msgid "File \"%(title)s\" was updated successfully."
msgstr "Plik \"%(title)s\" zaktualizowano pomyślnie."

#, python-format
msgid "File \"%(title)s\" could not be updated."
msgstr "Plik \"%(title)s\" nie mógł zostać zaktualizowany."

msgid "Downloading failed, the target object was not a folder!"
msgstr "Pobieranie zakończone niepowodzeniem, obiekt docelowy nie jest folderem!"

msgid "Canceled the download because there seem to be no files in this folder."
msgstr "Pobieranie anulowano, w folderze nie ma plików."

msgid "Internal Error: Group not Found"
msgstr "Błąd wewnętrzny: nie znaleziono Grupy"

msgid "Permission for upload denied"
msgstr "Odmowa zezwolenia na przesyłanie"

msgid "The uploaded file was invalid."
msgstr "Przesłany plik był nieprawidłowy."

#, python-format
msgid "%(count)d File was added successfully."
msgid_plural "%(count)d Files were added successfully."
msgstr[0] "%(count)d Plik dodano pomyślnie."
msgstr[1] "%(count)d Pliki dodano pomyślnie."
msgstr[2] "%(count)d Pliki dodano pomyślnie."

msgid "The file you uploaded was too large or the file type was not permitted to be uploaded!"
msgstr "Przesłany plik był zbyt duży lub typ pliku nie został dopuszczony do przesłania!"

