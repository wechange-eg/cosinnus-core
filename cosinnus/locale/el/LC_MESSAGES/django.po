msgstr[0] " 1 place left "
msgstr[1] " %(counter)s θέσεις κενές "

msgid "Collapse all processed applications"
msgstr "Σύμπτυξη όλων των αιτήσεων που έχουν υποβληθεί σε επεξεργασία"

msgid "not specified"
msgstr "δεν καθορίζεται"

msgid "show more ..."
msgstr "εμφάνιση περισσότερων..."

