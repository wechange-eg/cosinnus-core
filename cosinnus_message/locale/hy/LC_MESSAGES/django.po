msgid "Reply to all %(adressee_count)s"
msgstr "Պատասխանել բոլոր %(adressee_count)s -ին"

msgid "Your reply could not be matched to an existing message."
msgstr "Ձեր պատասխանը չի կարող համապատասխանեցվել առկա հաղորդագրությանը."

msgid "An internal error occured."
msgstr "Տեղի ունեցավ ներքին սխալ."

msgid "The email adress you sent the reply from is not the one associated with your user account. Please send direct replies only from the email adress you are registered with on the site!"
msgstr "Ձեր պատասխանը ուղարկվել է այն էլ. հասցեից, որը կապակցած է ձեր անձնական էջի հետ: Խնդրում ենք պատասխաններ ուղարկել միայն այն էլ․ հասցեից, որով դուք գրանցված եք կայքում!"

msgid "There was an error when processing your message text!"
msgstr "Ձեր հաղորդագրության տեքստը մշակելիս սխալ տեղի ունեցավ!"

msgid "Your direct reply failed!"
msgstr "Ձեր անմիջական պատասխանը ձախողվեց!"

msgid "Select at least one object."
msgstr "Ընտրեք առնվազն մեկ օբյեկտ."

msgid "Messages or conversations successfully archived."
msgstr "Հաղորդագրությունները կամ խոսակցությունները հաջողությամբ արխիվացվել են."

msgid "Messages or conversations successfully deleted."
msgstr "Հաղորդագրությունները կամ զրուցները հաջողությամբ ջնջվել են."

msgid "Messages were marked as read."
msgstr "Հաղորդագրությունները նշվել են որպես ընթերցված."

msgid "Messages or conversations successfully recovered."
msgstr "Հաղորդագրությունները կամ խոսակցությունները հաջողությամբ վերականգնվել են."

