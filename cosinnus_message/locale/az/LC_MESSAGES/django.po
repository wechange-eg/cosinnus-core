msgstr "'%(sender_first)s %(sender_last)s' sizin söhbətlərinizdən birinə mesaj göndərdi."

#, python-format
msgid "You have received a message from the user '%(sender_first)s %(sender_last)s'."
msgstr "Siz istifadəçi '%(sender_first)s %(sender_last)s' mesaj aldınız."

msgid "This message was sent in a conversation with: "
msgstr "Bu mesaj müvafiq söhbətlə göndərildi: "

msgid "Project/Group"
msgstr "Layihə / Qrup"

